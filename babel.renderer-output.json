{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "chrome >= 68"
                    ]
                },
                "useBuiltIns": false,
                "corejs": false,
                "forceAllTransforms": true,
                "bugfixes": true,
                "exclude": ["transform-typeof-symbol"]
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-class-properties"
    ]
}