{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "12.18.3"
                },
                "modules": "cjs"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-syntax-import-attributes",
            {
                "deprecatedAssertSyntax": true
            }
        ]
    ],
    "env": {
        "development": {
            "compact": false
        }
    }
}