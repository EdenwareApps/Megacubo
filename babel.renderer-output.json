{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "chrome >= 68"
                    ]
                },
                "useBuiltIns": false,
                "corejs": false,
                "forceAllTransforms": false,
                "bugfixes": false,
                "exclude": [
                    "transform-typeof-symbol",
                    "transform-for-of",
                    "transform-destructuring",
                    "transform-spread",
                    "transform-parameters"
                ]
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-class-properties"
    ]
}