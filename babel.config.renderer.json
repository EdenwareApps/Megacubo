{
    "allowAllFormats": true,
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "chrome >= 68"
                    ]
                },
                "useBuiltIns": false,
                "corejs": false,
                "modules": false,
                "exclude": [
                    "transform-typeof-symbol"
                ]
            }
        ]
    ],
    "plugins": [
        [
            "babel-plugin-transform-modules-iife", {
                "exportNamespace": "App"
            }
        ],
        "@babel/plugin-transform-class-properties"
    ]
}