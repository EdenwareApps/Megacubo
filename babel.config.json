{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "14.0.0"
                },
                "modules": "cjs"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-syntax-import-attributes",
            {
                "deprecatedAssertSyntax": true
            }
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "useESModules": false
            }
        ]
    ],
    "generatorOpts": {
        "retainLines": true,
        "compact": true,
        "minified": true
    },
    "env": {
        "development": {
            "compact": false
        }
    }
}